@model IEnumerable<WebApplication2.Models.ClienteViewModelTelanCarro>
@{
    ViewBag.Title = "Deleta";
}


<h2>Exclusão de carro vendido</h2>
<br /><br />
@using (Html.BeginForm("Deleta", "Home", FormMethod.Post))
{
    <p>por favor, favor digite o código do carro a ser excluído</p>
    <input id="Codigo" type="text" name="Codigo" value="" class="Data" />
    <input type="submit" name="btnOK" value="OK" />
}
<br/>


@foreach (var item in Model)
{


    <center>
        <div class="row">
            <a href="@item.foto1" data-lightbox="galeria" data-title="My caption">
                <div><img src="@item.foto1" style="width:60%; border-radius:10px ; margin-top:3px" height="400" />  </div>
            </a>
        </div>
    </center>
    <div style="width:100%;height:100px; background-color:gainsboro; margin-bottom:25px ; border-radius:10px"><center><h2><br /> digite esse codigo para deletar: @Html.DisplayFor(modelItem => item.descricao)</h2> </center></div>



}





