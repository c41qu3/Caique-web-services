@model IEnumerable<WebApplication2.Models.ClienteViewModelTelanCarro>

@{
    ViewBag.Title = "Lista";
}

<center><h2>Lista de carros na loja</h2></center>
 


@foreach (var item in Model)
{

    
    <center>
    <div class="row">
        <a href="@item.foto1" data-lightbox="galeria" data-title="My caption">
            <div class="col-md-6"><img src="@item.foto1" style="width:100%; border-radius:10px ; margin-top:3px" height="400" />  </div>
        </a>   
        <a href="@item.foto2" data-lightbox="galeria" data-title="My caption">
            <div class="col-md-6"><img src="@item.foto2" style="width:100%; border-radius:10px ; margin-top:3px" height="400" />  </div>
        </a>
   </div>
    </center>
    <div style="width:100%;height:100px; background-color:gainsboro; margin-bottom:25px ; border-radius:10px"><center><h2 ><br/>DESCRIÇÃO: @Html.DisplayFor(modelItem => item.descricao)</h2> </center></div>



}

